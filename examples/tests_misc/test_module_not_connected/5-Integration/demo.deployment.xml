    <deployedModuleInstance componentName="demoTestComponent" moduleInstanceName="myDemoEmitter_Connected_ext_AM_I" modulePriority="30"/>
    <deployedModuleInstance componentName="demoTestComponent" moduleInstanceName="myDemoEmitter_Unconnected_AM_I" modulePriority="30"/>
    <deployedModuleInstance componentName="demoTestComponent" moduleInstanceName="myDemoReceiver_Connected_AM_I" modulePriority="30"/>
    <deployedModuleInstance componentName="demoTestComponent" moduleInstanceName="myDemoReceiver_Connected_ext_AM_I" modulePriority="30"/>
    <deployedModuleInstance componentName="demoTestComponent" moduleInstanceName="myDemoReceiver_Unconnected_AM_I" modulePriority="30"/>
 </protectionDomain>

  <protectionDomain name="Dummy_PD">
    <executeOn computingNode="machine0" computingPlatform="Thales"/>
    <deployedModuleInstance componentName="demoDummyComponent" moduleInstanceName="myDummy_mod" modulePriority="30"/>
 </protectionDomain>

  <protectionDomain name="Dummy2_PD">
    <executeOn computingNode="machine0" computingPlatform="Thales"/>
