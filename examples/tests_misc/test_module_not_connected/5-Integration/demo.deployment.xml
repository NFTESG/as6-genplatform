    <deployedModuleInstance componentName="demoTestComponent_Cpp" moduleInstanceName="myDemoReceiver_Connected_ext_AM_I" modulePriority="30"/>
    <deployedModuleInstance componentName="demoTestComponent_Cpp" moduleInstanceName="myDemoReceiver_Unconnected_AM_I" modulePriority="30"/>
  </protectionDomain>

  <protectionDomain name="Finish_PD">
    <executeOn computingNode="machine0" computingPlatform="Thales"/>
    <deployedModuleInstance componentName="demoTestFinish" moduleInstanceName="myDemoFinish_AM_I" modulePriority="30"/>
  </protectionDomain>

  <platformConfiguration computingPlatform="Thales" faultHandlerNotificationMaxNumber="8">
    <computingNodeConfiguration computingNode="machine0" />
